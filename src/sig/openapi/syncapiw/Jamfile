SubDir ING_SRC sig openapi syncapiw ;

IISUBSYS sig openapi syncapiw ;

IISIGFILES syncapiw : apiconn.c apievent.c apiproc.c apiquery.c
	apisupp.c apitran.c sapiw.h
	sapiwi.h syncapiw.mak testsapi.c ;

IISIGFILES syncapiw : <sig!openapi!syncapiw>readme.txt ;

if $(VMS)
{
    IISIGSH syncapiw syncapiw : syncapiw$(SUFSH) ;
    IISIGSH sample_build syncapiw : sample_build$(SUFSH) ;
}
