  <alias name="ASMO-708" preferredBy="IANA JAVA"/>
  <alias name="8859_6" preferredBy="JAVA"/>
  <alias name="cp1089" preferredBy="JAVA"/>
  <alias name="1089" preferredBy="JAVA"/>
  <alias name="windows-28596" preferredBy="WINDOWS*"/>
  <alias name="ISO-8859-6-I" preferredBy="IANA MIME"/>
<!-- IANA considers this alias different and BiDi needs to be applied. -->
