# ASCII <-> NSCS
#
#	This is an identity mapping for 0..0x7f, holes otherwise.
#

local ASCII
remote NSCS
class THIRD

begin
    00=00, 01=01, 02=02, 03=03, 04=04, 05=05, 06=06, 07=07, # null
    08=08, 09=09, 0a=0a, 0b=0b, 0c=0c, 0d=0d, 0e=0e, 0f=0f, # \b\t\n\v\f\r
    10=10, 11=11, 12=12, 13=13, 14=14, 15=15, 16=16, 17=17,
    18=18, 19=19, 1a=1a, 1b=1b, 1c=1c, 1d=1d, 1e=1e, 1f=1f,
    20=20, 21=21, 22=22, 23=23, 24=24, 25=25, 26=26, 27=27, #  !@#$%&'
    28=28, 29=29, 2a=2a, 2b=2b, 2c=2c, 2d=2d, 2e=2e, 2f=2f, # ()*+,-./
    30=30, 31=31, 32=32, 33=33, 34=34, 35=35, 36=36, 37=37, # 01234567
    38=38, 39=39, 3a=3a, 3b=3b, 3c=3c, 3d=3d, 3e=3e, 3f=3f, # 89:;<=>?
    40=40, 41=41, 42=42, 43=43, 44=44, 45=45, 46=46, 47=47, # @ABCDEFG
    48=48, 49=49, 4a=4a, 4b=4b, 4c=4c, 4d=4d, 4e=4e, 4f=4f, # HIJKLMNO
    50=50, 51=51, 52=52, 53=53, 54=54, 55=55, 56=56, 57=57, # PQRSTUVW
    58=58, 59=59, 5a=5a, 5b=5b, 5c=5c, 5d=5d, 5e=5e, 5f=5f, # XYZ[\]^_
    60=60, 61=61, 62=62, 63=63, 64=64, 65=65, 66=66, 67=67, # `abcdefg
    68=68, 69=69, 6a=6a, 6b=6b, 6c=6c, 6d=6d, 6e=6e, 6f=6f, # hijklmno
    70=70, 71=71, 72=72, 73=73, 74=74, 75=75, 76=76, 77=77, # pqrstuvw
    78=78, 79=79, 7a=7a, 7b=7b, 7c=7c, 7d=7d, 7e=7e, 7f=7f, # xyz{|}~
end

