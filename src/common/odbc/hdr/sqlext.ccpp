XX/*----------------------------------------------*/
XX/* SQL_API_ODBC3_ALL_FUNCTIONS                  */
XX/* This returns a bitmap, which allows us to    */
XX/* handle the higher-valued function numbers.   */
XX/* Use  SQL_FUNC_EXISTS(bitmap,function_number) */
XX/* to determine if the function exists.         */
XX/*----------------------------------------------*/
XX
XX
XX#define SQL_API_ODBC3_ALL_FUNCTIONS   999
XX#define SQL_API_ODBC3_ALL_FUNCTIONS_SIZE 250 /* array of 250 words */
XX
XX#define SQL_FUNC_EXISTS(pfExists, uwAPI) \
XX ((*(((UWORD*) (pfExists)) + ((uwAPI) >> 4)) \
XX & (1 << ((uwAPI) & 0x000F)) \
XX  ) ? SQL_TRUE : SQL_FALSE \
XX )
