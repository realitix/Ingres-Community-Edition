XX#ifndef __SQLDATE
XX#define __SQLDATE
XX/* transfer types for DATE, TIME, TIMESTAMP */
XXtypedef struct tagDATE_STRUCT
XX{
XX        SQLSMALLINT    year;
XX        SQLUSMALLINT   month;
XX        SQLUSMALLINT   day;
XX} DATE_STRUCT;
XX
XXtypedef DATE_STRUCT   SQL_DATE_STRUCT;
XX
XXtypedef struct tagTIME_STRUCT
XX{
XX        SQLUSMALLINT   hour;
XX        SQLUSMALLINT   minute;
XX        SQLUSMALLINT   second;
XX} TIME_STRUCT;
XX
XXtypedef TIME_STRUCT   SQL_TIME_STRUCT;
XX
XXtypedef struct tagTIMESTAMP_STRUCT
XX{
XX        SQLSMALLINT    year;
XX        SQLUSMALLINT   month;
XX        SQLUSMALLINT   day;
XX        SQLUSMALLINT   hour;
XX        SQLUSMALLINT   minute;
XX        SQLUSMALLINT   second;
XX        SQLUINTEGER    fraction;
XX} TIMESTAMP_STRUCT;
XX
XXtypedef TIMESTAMP_STRUCT SQL_TIMESTAMP_STRUCT;
XX
XX
XX/*
XX * enumerations for DATETIME_INTERVAL_SUBCODE values for interval data types
XX * these values are from SQL-92
XX */
XX
XXtypedef enum 
XX{
XX      SQL_IS_YEAR             = 1,
XX      SQL_IS_MONTH            = 2,
XX      SQL_IS_DAY              = 3,
XX      SQL_IS_HOUR             = 4,
XX      SQL_IS_MINUTE           = 5,
XX      SQL_IS_SECOND           = 6,
XX      SQL_IS_YEAR_TO_MONTH    = 7,
XX      SQL_IS_DAY_TO_HOUR      = 8,
XX      SQL_IS_DAY_TO_MINUTE    = 9,
XX      SQL_IS_DAY_TO_SECOND    = 10,
XX      SQL_IS_HOUR_TO_MINUTE   = 11,
XX      SQL_IS_HOUR_TO_SECOND   = 12,
XX      SQL_IS_MINUTE_TO_SECOND = 13
XX} SQLINTERVAL;
XX
XX
XXtypedef struct tagSQL_YEAR_MONTH
XX{
XX      SQLUINTEGER     year;
XX      SQLUINTEGER     month;
XX} SQL_YEAR_MONTH_STRUCT;
XX
XXtypedef struct tagSQL_DAY_SECOND
XX{
XX      SQLUINTEGER     day;
XX      SQLUINTEGER     hour;
XX      SQLUINTEGER     minute;
XX      SQLUINTEGER     second;
XX      SQLUINTEGER     fraction;
XX} SQL_DAY_SECOND_STRUCT;
XX
XXtypedef struct tagSQL_INTERVAL_STRUCT
XX{
XX      SQLINTERVAL     interval_type;
XX      SQLSMALLINT     interval_sign;
XX      union 
XX      {
XX              SQL_YEAR_MONTH_STRUCT		year_month;
XX              SQL_DAY_SECOND_STRUCT		day_second;
XX      } intval;
XX} SQL_INTERVAL_STRUCT;
XX
XX
XX#endif        /* __SQLDATE    */
