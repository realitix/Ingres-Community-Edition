SubDir ING_SRC back opf specials_win ;

IISUBSYS back opf specials_win ;


IISHLIBRARY $(SHOPFLIBDATA) : $(SHOPFLIB) $(OPFLIBDATA) ;

if $(BUILD_IVW)
{
    IISHLIBRARY $(SHOPFLIB) : $(IMPOPFLIBDATA) $(OPFLIB) $(IMPCOMPATLIB) $(IMPCOMPATLIBDATA)
	$(IMPADFLIB) $(IMPADFLIBDATA) $(IMPPSFLIB) $(IMPSFLIBDATA) $(IMPRDFLIB) 
	$(IMPRDFLIBDATA) $(IMPULFLIB) $(IMPULFLIBDATA) $(IMPCUFLIB) $(IMPDMFLIB) 
	$(IMPDMFLIBDATA) $(IMPQSFLIB) $(IMPQSFLIBDATA) $(IMPEMBEDLIB) $(IMPEMBEDLIBDATA) 
	$(IMPSCFLIB) $(IMPSCFLIBDATA) $(X100LIB) $(X100CLIENTLIB) $(IMPQEFLIB) ;
}
else
{
    IISHLIBRARY $(SHOPFLIB) : $(IMPOPFLIBDATA) $(OPFLIB) $(IMPCOMPATLIB) $(IMPCOMPATLIBDATA)
	$(IMPADFLIB) $(IMPADFLIBDATA) $(IMPPSFLIB) $(IMPSFLIBDATA) $(IMPRDFLIB) 
	$(IMPRDFLIBDATA) $(IMPULFLIB) $(IMPULFLIBDATA) $(IMPCUFLIB) $(IMPDMFLIB) 
	$(IMPDMFLIBDATA) $(IMPQSFLIB) $(IMPQSFLIBDATA) $(IMPEMBEDLIB) $(IMPEMBEDLIBDATA) 
	$(IMPSCFLIB) $(IMPSCFLIBDATA) $(X100LIB) $(IMPQEFLIB) ;
}
