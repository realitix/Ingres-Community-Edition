# include	<compat.h>
# include	<gl.h>
# include	<sl.h>
# include	<iicommon.h>
# include	<fe.h>
# include	<st.h>
# include	<nm.h>
# include	<er.h>
# include	"imconst.h"
# include	"imextern.h"

VOID
vqlstart()
{
	char	*sp;
	char	buffer[80];
	char	*sysname = ERx("vql");
	char	*args[10];
	char	**argp = args;
	i4	argcount = 0;

	argp = args;
	argcount = 0;
	buffer[0] = EOS;

	NMgtAt(ERx("II_VQL_CONFIG"), &sp);
	if (sp != NULL && *sp)
	{
		STcopy(ERx("file = %S"), buffer);
		*argp++ = STalloc(sp);
		argcount++;
	}

	if (swspawn(sysname, buffer, argcount, args) != OK)
	{
		starterr(sysname);
	}
##	clear screen
}
