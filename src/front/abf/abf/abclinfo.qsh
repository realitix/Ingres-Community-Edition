typedef struct
{
	STATUS		(*func)();	/* function implementing action */
	ER_MSGID	menu;		/* menuitem message ID */
	ER_MSGID	expl;		/* menuitem explanation ID */
	i4		frskey;		/* frskey number or 0 */
} MENUS;

