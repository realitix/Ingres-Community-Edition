MODE on $(INGFILES)/rcfiles/iimgmtsvc.rc = $(FILEMODE) ;
IIRemRCComments $(INGFILES)/rcfiles/iimgmtsvc.rc : iimgmtsvc.rc ;

MODE on $(INGFILES)/rcfiles/iiinitdlib = $(FILEMODE) ;
IIRemRCComments $(INGFILES)/rcfiles/iiinitdlib : iiinitdlib.sh ;
