PART   uadt
   capability CI_USER_ADT

/*
** ingres!demo!udadts
*/

# ifdef FULL_RELEASE
#ifdef conf_LSB_BUILD
	DIRECTORY usr!share!ingres!demo!udadts
#else
	DIRECTORY ingres!demo!udadts
#endif
   BUILD_DIR (ING_BUILD)!demo!udadts
      FILE common.c
      FILE cpx.c
      FILE cpx.log
      FILE cpx.qry
      FILE iipk.h
      FILE depend.h
      FILE intlist.c
      FILE intlist.log
      FILE intlist.qry
      FILE numeric.c
      FILE numeric.txt
      FILE op.c
      FILE op_load.log
      FILE op_load.qry
      FILE op_test.log
      FILE op_test.qry
      FILE pnum_test.log
      FILE pnum_test.qry
      FILE udt.h
      FILE zchar.c
      FILE zchar.log
      FILE zchar.qry
      FILE nvarchar.c
      FILE nvarchar.log
      FILE nvarchar.qry
# ifdef VMS
      FILE common.obj
      FILE cpx.obj
      FILE iicvpk.obj
      FILE iimhpk.obj
      FILE intlist.obj
      FILE numeric.obj
      FILE op.obj
      FILE zchar.obj
      FILE nvarchar.obj
      SCRIPT build
      SCRIPT ccvms
# else /* VMS */ 
      FILE Makefile
      SCRIPT udttest
#if defined(axp_osf)
      FILE  libdemoudt.a
#else
#ifdef conf_LSB_BUILD
   DIRECTORY var!lib!ingres!demo!udadts
#endif /* LSB */
      EXECUTABLE libdemoudt.${slvers}.$SLSFX
#endif  /* axp_osf */
# endif /* VMS */
# endif /* FULL_RELEASE */

/*
** ingres!files
*/

#ifdef conf_LSB_BUILD
	DIRECTORY usr!share!ingres!files
#else
	DIRECTORY ingres!files
#endif
   BUILD_DIR (ING_BUILD)!files
      FILE iiadd.h
