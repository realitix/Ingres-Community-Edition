PART   abfdemo
   capability CI_ABF
   capability CI_VISION

/*
** ingres!abfdemo
*/

#ifdef conf_LSB_BUILD
	DIRECTORY usr!share!ingres!files!abfdemo
#else
	DIRECTORY ingres!files!abfdemo
#endif
   BUILD_DIR (ING_BUILD)!files!abfdemo
      FILE database.hlp
      FILE database.osq
      FILE delemp.osq
      FILE delemp.sql
      FILE dependen.dat
      FILE design.txt
      FILE emp.dat
      FILE empdep.hlp
      FILE empdep.osq
      FILE emptasks.hlp
      FILE emptasks.osq
      FILE experience.rw
      FILE iicopyapp.tmp
      FILE list.osq
      FILE projects.dat
      FILE qbf_forms.frm
      FILE remtask.osq
      FILE sqlcopy.in
      FILE startup.osq
      FILE sum_hours.sc
      FILE tasks.dat
      FILE timer_off.osq
      FILE timer_on.osq
      FILE top.hlp
      FILE top.osq

/*
** ingres!bin
*/

#ifdef conf_LSB_BUILD
	DIRECTORY usr!bin
#else
	DIRECTORY ingres!bin
#endif
   BUILD_DIR (ING_BUILD)!bin
      SCRIPT abfdemo
      SCRIPT deldemo
