*/

PART   netu

   BUILD_DIR (ING_BUILD)!sig!netu
# ifdef conf_LSB_BUILD
   DIRECTORY usr!libexec!ingres!sig!netu
# else
   DIRECTORY ingres!sig!netu
# endif
