/*
** Default directory: ingres/sig/star
*/

PART  sigstar

# ifdef VMS
   BUILD_DIR (sig_path)!star!star
# else /* VMS */
   BUILD_DIR (ING_BUILD)!sig!star
# endif /* VMS */
   DIRECTORY ingres!sig!star
# ifndef VMS
      FILE read.me
  {
   PERMISSION s:rwed,o:rwed,g:re,w:re,setuid
      EXECUTABLE wakeup
  }
      EXECUTABLE scheduler
      FILE makestdcat.sh
      FILE iialarm.frm
      SCRIPT iirunschd
# endif /* VMS */

