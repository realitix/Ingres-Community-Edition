PART   rmcmd

/*
** ingres!bin
*/

#ifdef conf_LSB_BUILD
	DIRECTORY usr!libexec!ingres!bin
#else
	DIRECTORY ingres!bin
#endif
   BUILD_DIR (ING_BUILD)!bin
# ifdef VMS
      SCRIPT rmcmd
      EXECUTABLE rmcmdv
# endif /* VMS */
      EXECUTABLE rmcmd
      EXECUTABLE rmcmdin
      EXECUTABLE rmcmdout
      EXECUTABLE rmcmdstp

/*
** ingres!utility
*/
 
#ifdef conf_LSB_BUILD
	DIRECTORY usr!libexec!ingres!utility
#else
	DIRECTORY ingres!utility
#endif
   BUILD_DIR (ING_BUILD)!utility
# ifdef VMS
      SCRIPT iirunrmcmd
# endif /* VMS */
      EXECUTABLE rmcmdrmv
      EXECUTABLE rmcmdgen

/*
** ingres!vdba
*/

#ifdef conf_LSB_BUILD
	DIRECTORY usr!share!ingres!vdba
#else
	DIRECTORY ingres!vdba
#endif
   BUILD_DIR (ING_BUILD)!vdba
# ifdef VMS
      FILE makimav.sql
# else
      FILE makimau.sql
# endif
