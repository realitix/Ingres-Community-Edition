IISUBSYS front st packman_unix_win ;

if $(BUILD_GTK_PACKMAN) = "TRUE"
{
    IINoHbUtilExe ingpackman : iipackman.c ;
    IINoHbNeedLibs ingpackman : GINSTALLLIB RFAPILIB COMPATLIB GTKLIBS ;
}

