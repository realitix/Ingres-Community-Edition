export II_SYSTEM=%rpm_prefix%
rc=0

if [ "$1" = "remove" ] ; then
    $II_SYSTEM/ingres/utility/iicorepreun -r %rpm_basename% \
        -v %rpm_version%
    rc=$?
elif [ "$1" = "upgrade" ] ; then
    $II_SYSTEM/ingres/utility/iicorepreun -r %rpm_basename% \
        -v %rpm_version% -u
    rc=$?

fi


exit $rc

