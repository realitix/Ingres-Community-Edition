static STATUS (*Metastamp)() = NULL;

/*
** metastamp registers a handler to be used to post-process every
** metaframe add.
*/
IICUmsMetaStamp(fn)
STATUS (*fn)();
{
	Metastamp = fn;
}

STATUS
IICUpaaPostAbfobjectAdd(class, level, name, ids)
OOID	class;
i4	level;
char	*name;
IDSTACK	*ids;
{
    STATUS	rval;

    switch(class)
    {
    case OC_BRWFRAME:
    case OC_UPDFRAME:
    case OC_APPFRAME:
    case OC_MUFRAME:
	if (Metastamp != NULL)
	    _VOID_ (*Metastamp)(ids->idstk[1]);

	/* FALL THROUGH */

    case OC_OSLPROC:
    case OC_OSLFRAME:

	if ( (rval = IIAMafrAddFidRecord(name, ids->idstk[1], ids->idstk[0])) 
		!= OK )
		return rval;
    }
    return OK;
}
