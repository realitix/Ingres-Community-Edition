? fill xbasis2.sql 
!!
create table qebldg (
	qebldg		char(2),
	qest_adr	char(30),
	qecity		char(15),
	qestate		char(2),
	qezip		char(5));
\g
create table qedept (
	qedname		char(8),
	qediv		char(3),
	qesales		float,
	qebldg		char(2),
	qefloor		smallint,
	qenum_emp	smallint);
\g
create table qeemp (
	qename		char(10),
	qesalary	float,
	qedept		char(8),
	qediv		char(3),
	qemgr		char(10),
	qebirthdate	char(25),
	qenum_dep	smallint);
\g
create table qesalhist (
	qename		char(10),
	qedate		char(25),
	qesalary	float);
\g
create table qereview (
	qename char(10),
	qerev_date char(25),
	qestrength char(250),
	qeweakness char(250),
	qegoals char(250));
\g
create table qesuppliers (
	qesno		char(5),
	qesname		char(20),
	qestatus	smallint,
	qecity		char(15));
\g
create view qelowpay (qename, qesalary)
	as select qename, qesalary
	from qeemp
	where qesalary < 30000;
\g
create view qeempinfo (qename, qedept, qediv, qebldg, qefloor)
	as select qeemp.qename, qeemp.qedept, qeemp.qediv,
		qedept.qebldg, qedept.qefloor
	from qeemp, qedept
	where qeemp.qedept = qedept.qedname and qeemp.qediv = qedept.qediv;
\g
create view qeyoungemp (qename, qesalary)
	as select qename, qesalary
	from qeemp
	where qebirthdate > '1/1/55';
\g
create table qeparts (
	qepno		char(6),
	qepname	        char(20),
	qecolor		char(6),
	qeweight	smallint,
	qecity		char(15));
\g
!!
