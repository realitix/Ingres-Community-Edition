	/* FRS mapping file for ANSI with Function Keys */

menu = pf40 (Esc)		/* Menu Key */
frskey1 = pf1 (F1)		/* Help Facility */
frskey2 = pf6 (F6)		/* Exit the system */
frskey3 = pf2 (F2)		/* Exit the frame, return to previous frame */
frskey4 = pf9 (F9)		/* Execute function */
frskey5 = controlY (Home)	/* Move to top of table field */
frskey6 = controlZ (End)	/* Move to bottom of table field */
frskey7 = pf5 (F5)		/* Search table field for specified string */
frskey8 = pf3 (F3)		/* Save object in database */
frskey9 = pf7 (F7)		/* Undo last action or forget chgs in frame */
frskey10 = pf8 (F8)		/* List the available choices for a field */
scrolldown = pf37 (PgUp)	/* Previous screen or set of rows in tbl fld */
scrollup = pf38 (PgDn)		/* Next screen or set of rows in table field */
menuitem1 = pf11 (SH-F1)	/* Select first menu item */
menuitem2 = pf12 (SH-F2)	/* Select second menu item */
menuitem3 = pf4 (F4)		/* Select third menu item - often 'Edit' */
menuitem4 = pf14 (SH-F4)	/* Select fourth menu item */
menuitem5 = pf15 (SH-F5)	/* Select fifth menu item */
menuitem6 = pf16 (SH-F6)	/* Select sixth menu item */
menuitem7 = pf17 (SH-F7)	/* Select seventh menu item */
menuitem8 = pf18 (SH-F8)	/* Select eighth menu item */
menuitem9 = pf19 (SH-F9)	/* Select ninth menu item */
menuitem10 = pf20 (SH-F10)	/* Select tenth menu item */
deletechar = pf39 (Delete)	/* Delete the character under the cursor */
nextitem = controlJ (Ctrl-Enter) /* Move to nxt fld without clearing this fld */
clearrest = controlM (Enter)	/* Clear rest of field & move to next fld */
shell = pf10 (F10)		/* Spawn a DOS shell */
