/* FRS mapping file for ANSI with Function Keys */

menu = pf34 (Esc)               /* Menu Key */

frskey1 = pf1 (F1)              /* Help Facility */

frskey2 = pf6 (F6)              /* Exit the system */

frskey3 = pf10 (F10)            /* Exit the frame, returning to previous frame */

frskey4 = pf9 (F9)              /* Execute function */

frskey5 = pf37 (Home)           /* Move to top of table field */

frskey6 = pf38 (End)            /* Move to bottom of table field */

frskey7 = pf5 (F5)              /* Search table field for specified string */

frskey8 = pf3 (F3)              /* Save object in database */

frskey9 = pf7 (F7)              /* Undo last action or forget changes made in frame */

leftchar = controlH (^H)        /* Move left one space within a field */

rightchar = controlL (^L)       /* Move right one space within a field */

scrollleft = controlO (^O)      /* Scroll left on a form */

scrollright = controlU (^U)     /* Scroll right on a form */

scrolldown = pf39 (PgUp)        /* Previous screen or set of rows in table field */

scrollup = pf40 (PgDn)          /* Next screen or set of rows in table field */

menuitem1 = pf11 (SH-F1)        /* Select first menu item */

menuitem2 = pf12 (SH-F2)        /* Select second menu item */

menuitem3 = pf13 (SH-F3)        /* Select third menu item */

menuitem4 = pf14 (SH-F4)        /* Select fourth menu item */

menuitem5 = pf15 (SH-F5)        /* Select fifth menu item */

menuitem6 = pf16 (SH-F6)        /* Select sixth menu item */

menuitem7 = pf17 (SH-F7)        /* Select seventh menu item */

menuitem8 = pf18 (SH-F8)        /* Select eighth menu item */

menuitem9 = pf19 (SH-F9)        /* Select ninth menu item */

menuitem10 = pf20 (SH-F10)      /* Select tenth menu item */

nextfield = controlI (Tab)      /* Move cursor to next field */

previousfield = controlP (SH-Tab)   /* Move cursor to previous field */

mode = pf35 (Ins)               /* Switch between insert and overstrike mode */

redraw = controlW (^W)          /* Redraw the screen */

deletechar = controlD (^D)      /* Delete the character under the cursor */

rubout = pf36 (Delete)	 	/* Delete character immediately to left of cursor */

editor = controlV (^V)          /* Start default text editor on field */

newrow = controlN (^N)          /* Move to first column of next row in table field */

clear = controlX (^X)           /* Clear the field */

duplicate = controlA (^A)       /* Auto-duplicate value while in fill mode */
